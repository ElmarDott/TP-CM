<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.8.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 https://maven.apache.org/xsd/decoration-1.8.0.xsd"
         name="Spring Framework BoM">

    <bannerLeft>
        <name>together Platform</name>
        <src>images/logo.gif</src>
        <href>index.html</href>
    </bannerLeft>

    <publishDate position="right"/>
    <version position="right"/>

    <body>
        <breadcrumbs>
            <item name="Home" href="https://together-platform.com"/>
        </breadcrumbs>

        <menu ref="parent" />
        <menu ref="modules" />
        <menu ref="reports" />

        <footer><![CDATA[<p style="text-align:center; font-weight:bold;">&copy; <span id="year"></span> <a href="https://elmar-dott.com" target="_blank">Elmar Dott Consulting</a></p>
<script>document.getElementById("year").innerHTML = new Date().getFullYear();</script>]]></footer>
    </body>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>2.0.0-M8</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>false</topBarEnabled>
            <sideBarEnabled>true</sideBarEnabled>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
        </fluidoSkin>
        <matomo>
            <siteId>together-platform</siteId>
            <url>https://analytics.elmar-dott.com</url>
            <options>
                <disableCookies />
                <trackPageView />
                <enableLinkTracking />
            </options>
        </matomo>
    </custom>
</project>
